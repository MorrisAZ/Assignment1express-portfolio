
<%- include partials/header.ejs %>

<%- include partials/main_nav.ejs %>


<!--main Content-->
<main class="container">
    <div class ="row">
        <div class ="col-md-offset-3 col-md-8">
            <h1><%= title %></h1>
            <br>
            <table class="table table-bordered table-striped table-hover">
        <!--Header Row-->
                <tr>
                    <th class="text-center">Name</th>
                    <th class="text-center">Phone</th>
                    <th class="text-center">Email:</th>
                </tr>

                <!--Repeatable row-->

                <% for (let count = 0; count < contactlist.lerngth; count ++){ %>
                    <tr>
                        <td class = "text-center"><%= BusinessContactList[count].name %></td>
                        <td class = "text-center"><%= BusinessContactList[count].phone %></td>
                        <td class = "text-center"><%= BusinessContactList[count].email %></td>
                    </tr>
            </table>    
        </div>
    </div>
</main>





